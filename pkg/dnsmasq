[deps]

[build]
make PREFIX="$butch_prefix" CFLAGS="$optcflags" LDFLAGS="$optldflags" -j$MAKE_THREADS || exit 1
make BINDIR="$butch_prefix"/bin PREFIX="$butch_prefix" DESTDIR="$butch_install_dir" install || exit 1
install -D -m 640 dnsmasq.conf.example "$butch_install_dir"/"$butch_prefix"/etc/dnsmasq.conf || exit 1
