[build]
LDFLAGS="-static" CFLAGS="-include stdio.h -include stdlib.h" ./configure --prefix="$butch_prefix" \
  --disable-shared --disable-dso || exit 1

make -j$MAKE_THREADS || exit 1
make DESTDIR="$butch_install_dir" install || exit 1
