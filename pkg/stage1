[deps]
kbd
9base
busybox
e2fsprogs
gcc4
gmp
mpc
mpfr
musl
dynamic-toolchain
man
zlib

[build]
rm -rf "$butch_root_dir"/opt/gcc3
rm -rf "$butch_root_dir"/opt/stage0_musl
rm -rf "$butch_root_dir"/opt/stage0
rm -rf "$butch_root_dir"/opt/stage1

butch unlink gcc3
butch relink `ls -d "$butch_root_dir"/opt/gcc4* |awk -F/ '{print $NF}' |tail -n1`
